{
  "name": "padatika",
  "version": "0.0.1",
  "description": "A customizable footnote system inspired by wikipedia.",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --minify --sourcemap --dts"
  },
  "keywords": [
    "footnotes",
    "endnotes",
    "citations",
    "references",
    "notes"
  ],
  "author": "Ashutosh Biswas",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  }
}
